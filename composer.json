{
  "name": "xwpd/thinkphp-apidoc-generator",
  "license": "MIT",
  "description": "Generate beautiful API documentation from your ThinkPHP application",
  "keywords": [
    "API",
    "Documentation",
    "ThinkPHP"
  ],
  "homepage": "http://github.com/xwpd/thinkphp-apidoc-generator",
  "authors": [
    {
      "name": "xwpd",
      "email": "514261300@qq.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "fzaninotto/faker": "~1.0",
    "topthink/framework": "5.1.*",
    "mpociot/documentarian": "^0.2.0",
    "mpociot/reflection-docblock": "^1.0",
    "ramsey/uuid": "^3.0",
    "terranc/think-blade": "^1.1"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": "~4.0 || ~5.0",
    "mockery/mockery": "^0.9.5"
  },
  "autoload": {
    "psr-4": {
      "Xwpd\\ThinkApiDoc\\": "src/Xwpd/ThinkApiDoc"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xwpd\\ThinkApiDoc\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias":{
      "dev-master":"0.x.x-dev"
    },
    "think": {
      "services": [
        "Xwpd\\ThinkApiDoc\\ApiDocGeneratorServiceProvider"
      ]
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
